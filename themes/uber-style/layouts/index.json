{{- $.Scratch.Add "index" slice -}}
{{- range site.RegularPages -}}
    {{- $.Scratch.Add "index" (dict "title" .Title "content" .Plain "permalink" .Permalink "summary" (.Params.summary | default .Summary) "date" .Date "tags" .Params.tags "categories" .Params.categories) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}